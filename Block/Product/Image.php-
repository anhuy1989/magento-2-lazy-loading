namespace Mageplaza\LazyLoading\Block\Product;
use Mageplaza\LazyLoading\Helper\Data as HelperData;
use Mageplaza\LazyLoading\Helper\Image as HelperImage;

class Image extends \Magento\Catalog\Block\Product\Image
{
    protected $helperData;
    protected $helperImage;
    public function __construct(
        \Magento\Framework\View\Element\Template\Context $context,
        HelperData $helperData,
        HelperImage $helperImage,
        array $data = []
    ) {
        $this->helperData = $helperData;
        $this->helperImage = $helperImage;
        if (isset($data['template'])) {
            $this->setTemplate($data['template']);
            unset($data['template']);
        }
        parent::__construct($context, $data);
    }
}